<div class="file-select-control">
	<div role="tabpanel" class="tab-pane active" id="download">
		<form class="form-inline">
			<div class="tags">
				{{> filebrowser/tags}}
			</div>

			<div class="file-selector"></div>

			<div class="buttons">
				<input id="file-url"
					class="form-control pull-left"
					size="45"
					type="text"
					value="{{original}}"
					placeholder=""/>
			</div>
		</form>
	</div>
</div>
