<div class="searchbox">
	<input id="presetSearch"
		class="form-control"
		type="text"
		placeholder="Search presets and plugins"/>
</div>

<div class="preset-result"></div>

<div class="panel-group selectControl" id="{{controlId}}">
	{{#each categories}}
	<div class="panel panel-default">
		<div class="panel-heading"
			data-toggle="collapse"
			data-category="{{@key}}"
			data-parent="#{{../controlId}}"
			href="#cscat{{@index}}">
				<i class="glyphicon glyphicon-chevron-right"></i>
				{{@key}}
		</div>
	
		<div id="cscat{{@index}}"
			class="panel-collapse collapse">

			<div class="panel-body">
				<ul>
					{{#each this}}
					<li 
						data-category="{{category}}"
						data-path="{{path}}"
					>
					<div class="score"></div>
					{{title}}
					</li>
					{{/each}}
				</ul>
			</div>
		</div>
	</div>
	{{/each}}

</div>
